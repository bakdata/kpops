{
    "categories": [
        {"title": "## 🚀 Features", "labels": ["feature", "feat", "enhancement"]},
        {"title": "## 🐛 Fixes", "labels": ["fix", "bug"]},
        {"title": "## 🧪 Dependencies", "labels": ["dependency"]}
    ],
    "ignore_labels": ["ignore"],
    "sort": {"order": "ASC", "on_property": "mergedAt"},
    "template": "# [${{TO_TAG}}](https://github.com/bakdata/kpops/releases/tag/${{TO_TAG}}) - Release Date: ${{TO_TAG_DATE}}\n\n${{CHANGELOG}}\n\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n<\n",
    "pr_template": "- ${{TITLE}} - [#${{NUMBER}}](${{URL}})\n",
    "empty_template": "- no changes!"
}
