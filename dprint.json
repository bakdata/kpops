{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "incremental": true,
  "markdown": {},
  "toml": {},
  "malva": {},
  "includes": [
    "**/*.{md}",
    "**/*.{toml}",
    "**/*.{css}"
  ],
  "excludes": [
    ".pytest_cache/**",
    ".mypy_cache/**",
    "**/.venv/**",
    "CHANGELOG.md",
    "**/cli-commands.md",
    "**/cli_env_vars.md",
    "**/config_env_vars.md"
  ],
  "plugins": [
    "https://plugins.dprint.dev/markdown-0.16.0.wasm",
    "https://plugins.dprint.dev/toml-0.5.4.wasm",
    "https://plugins.dprint.dev/g-plane/malva-v0.1.2.wasm"
  ]
}
