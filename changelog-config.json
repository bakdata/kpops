{
    "categories": [
        {
            "title": "## üöÄ Features",
            "labels": [
                "feature",
                "feat"
            ]
        },
        {
            "title": "## üêõ Fixes",
            "labels": [
                "fix",
                "bug"
            ]
        },
        {
            "title": "## üß™ Dependencies",
            "labels": [
                "dependency"
            ]
        }
    ],
    "ignore_labels": [
        "ignore"
    ],
    "sort": {
        "order": "ASC",
        "on_property": "mergedAt"
    },
    "template": "# [${{TO_TAG}}](https://github.com/bakdata/kpops/releases/tag/${{TO_TAG}}) - ${{TO_TAG_DATE}}\n\n${{CHANGELOG}}\n<details>\n<summary>Uncategorized</summary>\n\n${{UNCATEGORIZED}}\n</details>\n",
    "pr_template": "- ${{TITLE}}\n   - PR: ${{URL}}\n   - Assignees: ${{ASSIGNEES[*]}}\n   - Reviewers: ${{REVIEWERS[*]}}\n   - Approvers: ${{APPROVERS[*]}}",
    "empty_template": "- no changes!"
}
